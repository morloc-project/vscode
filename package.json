{
  "name": "morloc",
  "displayName": "morloc",
  "description": "VS Code support for morloc",
  "publisher": "morloclang",
  "icon": "icons/morloc-icon.png",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.78.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/morloc-project/vscode.git"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "keywords": [
    "morloc",
    "morloc language"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "morloc",
        "path": "snippets/snippets.json"
      }
    ],
    "languages": [{
      "id": "morloc",
      "aliases": ["morloc"],
      "extensions": [".loc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "morloc",
      "scopeName": "source.morloc",
      "path": "./syntaxes/morloc.tmLanguage.json"
    }]
  }
}
